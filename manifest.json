{
  "manifest_version": 3,
  "name": "115PC Auto Cookie Loader",
  "version": "1.0.0",
  "description": "115瀏覽器 cookie 自動加載工具",
  "permissions": ["cookies", "tabs"],
  "host_permissions": ["https://115.com/*"],
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "resources": ["cookie.json"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "128": "icons/icon128.png"
  }
}
